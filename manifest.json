{
  "manifest_version": 3,
  "name": "GCU - Classroom Utilities",
  "description": "An extension to extend the functionality of google classroom, and make it easier to manage your classroom",
  "version": "1.0",
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["https://classroom.google.com/*"],
      "js": ["content-script.js"],
      "css": ["content-script.css"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "key": "",
  "oauth2": {
    "client_id": "825544639595-8segfm42rq9ocgjpli85k5angrf6d49p.apps.googleusercontent.com",
    "scopes":[
      "https://www.googleapis.com/auth/classroom.courses.readonly",
      "https://www.googleapis.com/auth/classroom.rosters.readonly"
    ]
  },
  "permissions": [
    "identity",
    "tabs",
    "storage"
  ]
}